<h3>Listado de Sectoriales</h3>
<ol class="breadcrumb">
  <li>
    <%= link_to 'Inicio', root_path %>
  </li>
  <li>
    Sectoriales
  </li>
  <li>
    <strong><%= link_to 'AÃ±adir Sectorial', new_sectorial_path %></strong>
  </li>
</ol>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Ip address</th>
      <th>Limite</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sectorials.each do |sectorial| %>
      <tr>
        <td><%= sectorial.nombre %></td>
        <td><%= sectorial.ip_address %></td>
        <td><%= sectorial.limite %></td>
        <td><%= link_to 'Show', sectorial %></td>
        <td><%= link_to 'Edit', edit_sectorial_path(sectorial) %></td>
        <td><%= link_to 'Destroy', sectorial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h1>Listing Ip Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ip_statuses.each do |ip_status| %>
      <tr>
        <td><%= ip_status.status %></td>
        <td><%= link_to 'Show', ip_status %></td>
        <td><%= link_to 'Edit', edit_ip_status_path(ip_status) %></td>
        <td><%= link_to 'Destroy', ip_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ip status', new_ip_status_path %>
